#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/macbookairm2/Project/desarrolladoresMSPAS/hermesInterface/node_modules/.pnpm/angular-cli-ghpages@2.0.3/node_modules/angular-cli-ghpages/node_modules:/Users/macbookairm2/Project/desarrolladoresMSPAS/hermesInterface/node_modules/.pnpm/angular-cli-ghpages@2.0.3/node_modules:/Users/macbookairm2/Project/desarrolladoresMSPAS/hermesInterface/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/macbookairm2/Project/desarrolladoresMSPAS/hermesInterface/node_modules/.pnpm/angular-cli-ghpages@2.0.3/node_modules/angular-cli-ghpages/node_modules:/Users/macbookairm2/Project/desarrolladoresMSPAS/hermesInterface/node_modules/.pnpm/angular-cli-ghpages@2.0.3/node_modules:/Users/macbookairm2/Project/desarrolladoresMSPAS/hermesInterface/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../angular-cli-ghpages" "$@"
else
  exec node  "$basedir/../../angular-cli-ghpages" "$@"
fi
